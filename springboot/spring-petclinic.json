{
  "id": "/petclinic",
  "cmd": "export PATH=$PATH:/opt/mesosphere/bin;\nexport SERVER_PORT=$PORT0;\ngit clone https://github.com/spring-projects/spring-petclinic.git &&\ncd spring-petclinic &&\n./mvnw spring-boot:run",
  "instances": 1,
  "cpus": 2,
  "mem": 4096,
  "disk": 0,
  "gpus": 0,
  "constraints": [],
  "fetch": [],
  "storeUrls": [],
  "backoffSeconds": 1,
  "backoffFactor": 1.15,
  "maxLaunchDelaySeconds": 3600,
  "container": {
    "volumes": [],
    "type": "MESOS"
  },
  "healthChecks": [],
  "readinessChecks": [],
  "dependencies": [],
  "upgradeStrategy": {
    "minimumHealthCapacity": 1,
    "maximumOverCapacity": 1
  },
  "unreachableStrategy": {
    "inactiveAfterSeconds": 300,
    "expungeAfterSeconds": 600
  },
  "killSelection": "YOUNGEST_FIRST",
  "portDefinitions": [
    {
      "port": 10101,
      "protocol": "tcp"
    }
  ],
  "requirePorts": true,
  "labels": {
    "HAPROXY_GROUP": "external",
    "HAPROXY_0_PORT": "10000",
    "HAPROXY_INDEX_PORT": "0"
  }
}
